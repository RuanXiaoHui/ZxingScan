<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ViewfinderView">
        <!--扫描边框的宽度-->
        <attr name="inner_width" format="dimension"/>
        <!--扫描边框的高度-->
        <attr name="inner_height" format="dimension"/>
        <!--扫描边框距离顶部的margin-->
        <attr name="inner_margintop" format="dimension"/>
        <!--扫描边框的颜色-->
        <attr name="inner_corner_color" format="color"/>
        <!--扫描边框四个角的长度-->
        <attr name="inner_corner_length" format="dimension"/>
        <!--扫描边框四个角的厚度-->
        <attr name="inner_corner_width" format="dimension"/>
        <!--扫描线的速度-->
        <attr name="inner_scan_speed" format="dimension"/>
        <!--星星是否显示-->
        <attr name="inner_scan_iscircle" format="boolean"/>
        <!--扫描线的颜色-->
        <attr name="inner_scan_line_color" format="color"/>
        <!--扫描线是都显示，默认是不显示的-->
        <attr name="inner_scan_line_isShow" format="boolean"/>
        <!--扫描线的高度-->
        <attr name="inner_scan_line_height" format="dimension"/>
        <!--扫描区域提示文本-->
        <attr name="label_text" format="string|reference"/>
        <!--扫描区域提示文本颜色-->
        <attr name="label_text_color" format="color|reference"/>
        <!--扫描区域提示文本Size-->
        <attr name="label_text_size" format="dimension"/>
    </declare-styleable>

</resources>